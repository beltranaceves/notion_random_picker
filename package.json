{
  "name": "Notion Database random picker",
  "description": "Randomly pick any item from a Notion database on any view",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "dev:chrome": "wxt -b chrome",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:chrome": "wxt build -b chrome",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:chrome": "wxt zip -b chrome",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "submit:init": "wxt submit init"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "wxt": "^0.19.1"
  }
}
